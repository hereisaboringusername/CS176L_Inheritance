

package labproject6.persons;

public class Faculty extends Employee{
	//Insert  your instance variable. Faculty has instance variable of rank.
	private String rank;

	//This sets the rank of the faculty member
	public void setRank (String rank) {
		this.rank = rank;
	}

	@Override
	public String toString() {
		return super.toString() + "\n" +
				"Rank: " + rank;
	}

}
