
/**
 * Superclass 
 * Sets the name of each person
 * @author Ashley Z.
 *
 */
package labproject6.persons;
public class Person {
	//instance variables
	private String name;

	//Methods: 
	//This method sets the name. 
	public void setName(String name) {
		this.name = name;
	}

	public String toString() {
		return getClass() + "\n" + 
				"Name: " + name;
	}
}

